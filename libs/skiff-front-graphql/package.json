{
  "name": "skiff-front-graphql",
  "version": "0.0.1",
  "description": "Frontend GQL Utils",
  "author": "Skiff World Inc.",
  "license": "CC-BY-NC-4.0",
  "main": "./dist/cjs/index.js",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "types": "dist/src/index.d.ts",
  "packageManager": "yarn@3.5.0",
  "scripts": {
    "emit-ts": "tsc --emitDeclarationOnly",
    "clean": "rm -rf dist/",
    "build": "yarn codegen && yarn node build.js",
    "watch": "WATCH=1 yarn build",
    "codegen": "yarn graphql-codegen --config codegen.yml",
    "lint": "yarn run -T eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn lint --fix"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-operations": "^2.3.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.8",
    "@graphql-codegen/typescript-resolvers": "^2.5.2",
    "@types/jest": "^28.1.1",
    "@types/node": "^18.7.18",
    "@types/semver": "^7",
    "@types/uuid": "^8.3.4",
    "@types/varint": "^6.0.0",
    "concurrently": "^7.1.0",
    "esbuild": "^0.14.39",
    "esbuild-jest": "^0.5.0",
    "esbuild-node-externals": "^1.5.0",
    "eslint-config-skiff-eslint": "workspace:*",
    "jest": "^28.1.1",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "notistack": "^2.0.3",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@skiff-org/skiff-crypto": "workspace:libs/skiff-crypto",
    "@spliterati/threshold": "^0.4.2",
    "@stablelib/base64": "^1.0.1",
    "@stablelib/chacha20poly1305": "^1.0.1",
    "base64-js": "^1.5.1",
    "buffer": "^6.0.3",
    "fflate": "^0.7.3",
    "graphql": "16.5.0",
    "long": "^5.2.0",
    "protobufjs": "^6.11.3",
    "semver": "^7.3.5",
    "skiff-graphql": "workspace:libs/skiff-graphql",
    "skiff-mail-protos": "workspace:libs/skiff-mail-protos",
    "skiff-utils": "workspace:libs/skiff-utils",
    "ts-proto": "^1.105.1",
    "uuid": "^8.3.2",
    "varint": "^6.0.0"
  }
}
