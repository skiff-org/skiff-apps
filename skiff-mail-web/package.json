  "name": "skemail-web",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@4.0.1",
  "scripts": {
    "analyze": "OPEN_ANALYZER=true yarn build",
    "build": "yarn build:clean && yarn build-main && yarn package-push-sw",
    "build-vercel": "yarn build:clean && cross-env NODE_ENV=production dotenv -c vercel --  yarn webpack --config ./configs/webpack.config.web.prod.babel.js",
    "build:clean": "rm -rf src/dist",
    "codegen": "yarn graphql-codegen --config codegen.yml",
    "dev": "yarn start-dev",
    "ts": "yarn tsc --noEmit",
    "lint": "yarn run -T eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "lint:cycles": "yarn lint --color=true | grep -B 1 \"cycle\"",
    "lint:fix": "yarn lint --fix",
    "start-dev": "cross-env NODE_ENV=development dotenv -- yarn webpack serve --no-client-overlay-warnings --config ./configs/webpack.config.web.dev.babel.js",
    "start-dev-e2e": "cross-env NODE_ENV=development dotenv -- yarn webpack serve --no-client-overlay-warnings --config ./configs/webpack.config.web.dev.babel.js",
    "serve-dist": "yarn node internals/serve-dist.js",
    "test": "yarn ts && NODE_OPTIONS=--max-old-space-size=4096 yarn jest --maxWorkers=2 --logHeapUsage",
    "test-watch": "yarn test --watch",
    "build-main": "cross-env NODE_ENV=production yarn webpack --config ./configs/webpack.config.web.prod.babel.js",
    "package-push-sw": "rm ./src/dist/push/push-sw* && cp ./src/push/push-sw.js ./src/dist/push/push-sw.js",
    "webpack:clean": "rm -rf .yarn/.cache",
    "skemail-front:start-dev-local": "dotenv -- yarn webpack serve --config configs/webpack.config.web.dev.babel.js"
  },
